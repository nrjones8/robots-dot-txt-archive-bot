        var autoPlayTimer = null;
        var mbOptions = null;
        var videoHasPlayed = false;
        var restartTimer = 0;
        var captionsSelector = null;
        var videoFilesObj = [{height: 144,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898-256x144-111k.mp4"},{height: 288,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898-512x288-442k.mp4"},{height: 576,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898-1024x576-1769k.mp4"},{height: 720,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898-1280x720-2765k.mp4"},{height: 1080,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898-1920x1080-6221k.mp4"},{height: 1080,src: "https://cdn.dvidshub.net/media/video/2010/DOD_108023898/DOD_108023898.mp4"}];
        var currentVideoHeight = 576;
