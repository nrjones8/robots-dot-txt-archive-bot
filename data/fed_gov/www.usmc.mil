<script type="text/javascript">
    // OnPointAnnouncer - only one such component on page should be present
    // @NOTE: Requires GSAP Animation Platform, add TweenMax Library for smooth animations
    // Add either via Node or via SharedLibrary to use this component

    var OnPointAnnouncer = OnPointAnnouncer || function(config) {
      if (!config && !config.id && !config.btnCloseId)  return;
      var element = document.getElementById(config.id);
      var announcer = document.getElementById(config.announcerId);
      var close = document.getElementById(config.btnCloseId);
      if (!element && !announcer && !close) return;
      var timeout;

      function hide(e) {
        var target = (e.currentTarget) ? e.currentTarget : e.srcElement;
        var currentSlide = $(target).closest('.dma-slideshow.onpoint');
        currentSlide.removeClass('with-on-point');
        if (carousel && config.autoRotate) {
          carousel.pause();
          if (config.stopRotation) {
            currentSlide.off('mouseover', '**');
            currentSlide.off('mouseout', '**');
          }
        }
      }

      function open() {
        window.setTimeout(function() {
          $('.dma-slideshow.onpoint').addClass('with-on-point');
        }, 3500);
      }

      if (close) {
        close.addEventListener('click', hide, false);
        close.addEventListener('touch', hide, false);
      }

      var carousel = new OnPointCarousel(config);
      if (carousel && config.autoRotate) {
        if (config.stopRotation) {
          element.addEventListener('mouseover', function() { carousel.pause(); }, false);
          element.addEventListener('mouseout', function() { carousel.rotate(); }, false);
        }

        window.addEventListener('resize', function() { // debouncer
          carousel.pause();
          if (timeout) clearTimeout(timeout);
          timeout = setTimeout(function() {
            carousel.rotate();
          }, 12000);
        }, false);
        carousel.rotate();
      }

      open();
    };

    var OnPointCarousel = OnPointCarousel || function(config) {
      var strip, pager, leftControl, rightControl, current, quantity, autoRotate, timer, distance;
      strip = config.carouselId ? document.getElementById(config.carouselId) : null;
      pager = config.pagerId ? document.getElementById(config.pagerId) : null;
      leftControl = config.btnLeft ? document.getElementById(config.btnLeft) : null;
      rightControl = config.btnRight ? document.getElementById(config.btnRight) : null;
      autoRotate = config.autoRotate || false;
      rotateDuration = config.rotateDuration || 5000; // milliseconds
      transitionDuration = config.transitionDuration || 1000; // milliseconds
      if (!strip || !leftControl || !rightControl || !pager) return;

      function setup() {
        quantity = strip.children.length;
        if (quantity < 2) {
          leftControl.parentNode.style.display = "none";
          pager.style.display = "none";
          autoRotate = false;
        } else {
          observe();
        }
        current = 0;
        distance = 100 / quantity;
        strip.style.width = quantity * 100 + '%';
        strip.style.minWidth = quantity * 100 + '%';
        pager.innerHTML = (current + 1) + ' of ' + quantity;
        for (var i = 0; i < quantity; i++) {
          strip.children[i].style.width = distance + '%';
          strip.children[i].style.maxWidth = distance + '%';
        }
      }

      function moveLeft() {
        if (current + 1 <= 0) {
          ++current;
        } else if (current + 1 == 1) {
          current = -quantity + 1;
          TweenMax.set(strip, {x:  (-quantity * distance)+ '%'});
        }
        TweenMax.to(strip, transitionDuration/1000, {
          x: (current * distance) + '%',
          ease: 'cubic-bezier(0.4, 0.0, 0.2, 1)',
          onComplete: function(){
            pager.innerHTML = (Math.abs(current) + 1) + ' of ' + quantity;
          }
        });
      }

      function moveRight() {
        if (Math.abs(current - 1) < quantity) {
          current--;
        } else if (Math.abs(current - 1) == quantity) {
          current = 0;
          TweenMax.set(strip, {x: distance + '%'});
        }
        TweenMax.to(strip, transitionDuration/1000, {
          x: (current * distance) + '%',
          ease: 'cubic-bezier(0.4, 0.0, 0.2, 1)',
          onComplete: function(){
            pager.innerHTML = (Math.abs(current) + 1) + ' of ' + quantity;
          }
        });
      }

      function rotate() {
        var totalTime = transitionDuration + rotateDuration;
        timer = window.setInterval(function(){
          moveRight();
        }, totalTime);
      }

      function pause() {
        window.clearInterval(timer);
      }

      function observe() {
        leftControl.addEventListener('click', moveLeft, false);
        rightControl.addEventListener('click', moveRight, false);
        leftControl.addEventListener('touch', moveLeft, false);
        rightControl.addEventListener('touch', moveRight, false);
        jQuery(strip).swipe({
          swipeLeft: function(event, direction, distance, duration, fingerCount, fingerData) {
            console.log('swipe left');
            moveRight();
          },
          swipeRight: function(event, direction, distance, duration, fingerCount, fingerData) {
            console.log('swipe right');
            moveLeft();
          }
        });
      }

      setup();

      return {
        pause: pause,
        rotate: rotate
      };
    };

    // add backend integration to check for slides
    document.addEventListener('DOMContentLoaded', function() {
