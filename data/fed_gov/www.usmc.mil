function doSearch(val) {
  //grab the text, strip out # and &
  var searchtxt = encodeURI(val.trim().replace(/#|&/g, ""));
  if(searchtxt){
    //redirect to /Home/Search
    window.location.assign("/Home/Search?tag=" + searchtxt);
  }
}
</script>



