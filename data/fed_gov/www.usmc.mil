      @var dvidsobject - Object Literal
    */
    var DvidsAPI = {
        currentPlayer:null,

        selectVideoFile:function(files) {
            var currentWidth = 0, pick;
            for (var j = 0, jlen = files.length; j < jlen; j++) {
                if (files[j].width == Config.Video.WIDTH) {
                    pick = files[j];
                }
            }
