					// Also change aria-expanded
					$(this).parents().children("a.left-nav-icon").attr("aria-expanded", true);

					// Make it look like active
					//$(this).addClass("active");

					// If one match is found, get out, that is all we need
					return false;
				}
			});

			//	Drop filename and/or trailing slash
			function dropFilename(URL)
			{
				if (URL != null && URL !== undefined && URL.length > 0)
				{
					var i = URL.lastIndexOf("/");

					if (i < 0)							// has no slash, e.g. "index.html"
						return URL;

					if (i == URL.length - 1)			// slash is last character, e.g. "/about/careers/"
						return URL.substring(0, i);

					if (URL.lastIndexOf(".html") > -1)	// has filename, e.g. "/about/careers/index.html"
						return URL.substring(0, i);

					return URL;							// has no filename, e.g. "/about/careers"
				}

				return "";
			}


			/*
				A - Z left nav && Main Menu (The Top 4)
			*/

			var currentrawURL = window.location.href;

			var n = currentrawURL.indexOf("//");
			var cleanURL = currentrawURL.substring(n+2);

			var m = cleanURL.indexOf("/");

			if (m != (cleanURL.length - 1)) {
				var azURL = cleanURL.substring(m+1);
				var URLparts = azURL.split("/");

				if (URLparts.length > 1 && URLparts[1] != "") {
					if (URLparts[1] == "numbers") {
					   var azTarget = "#";
					} else {
					   azTarget = URLparts[1].toUpperCase();
					}


					if (URLparts[0] == "az") {
					   $(".AZlist-main").find("a:contains('" + azTarget + "')").addClass("Selected");
					}
				}

				switch (URLparts[0]) {
					/*
					ASH Main Menu (active state)
				    */
				  case "ash":
				    if (URLparts.length > 1 && URLparts[1] != "") {
						switch (URLparts[1]) {
							case "about-ash":
							  $(".nav-about-ash").addClass("active");
							  break;
							case "programs":
							  $(".nav-public-health-offices").addClass("active");
							  break;
							case "advisory-committees":
							  $(".nav-advisory-committees").addClass("active");
							  break;
						}
					}
				   break;
				   case "nvpo":
				    /*
				  	NVPO Main Menu (active state)
				    */
					  if (URLparts.length > 1 && URLparts[1] != "") {
						switch (URLparts[1]) {
							case "about":
							  $(".nav-about-nvpo").addClass("active");
							  break;
							case "featured-priorities":
							  $(".nav-featured-priorities").addClass("active");
							  break;
							case "nvac":
							  $(".nav-nvac").addClass("active");
							  break;
							case "national-vaccine-plan":
							  $(".nav-nvp").addClass("active");
							  break;
							case "national-adult-immunization-plan":
							  $(".nav-naip").addClass("active");
							  break;
						}
					  }
					break;
					case "ohaidp":
					/*
					OHAIDP Main Menu (active state)
				    */
		              if (URLparts.length > 1 && URLparts[1] != "") {
						switch (URLparts[1]) {
							case "initiatives":
							  $(".nav-initiatives").addClass("active");
							  break;
							case "resources":
							  $(".nav-resources").addClass("active");
							  break;
							case "contact-us":
							  $(".nav-contact-us").addClass("active");
							  break;
						}
					  }
					break;
				}
			}

		})(jQuery);
		</script>
        
