	<script>
	maxscroll = 0;
	var scrollem = "";
	feedindex = 0;
	canclick=true;
	$(function() {
		$(document).on("click",".feedclick", function() {
			gohere = $(this).data("linktag");
			window.open(gohere, "_blank");
		});
		$("#gonow").click(function() { 
			$("#wheretoservefoot").submit();
		});
		$(".locator").keyup(function(e) { 
			if (e.charCode == 13) {
				$("#wheretoservefoot").submit(); 
			}
		}); 
		$(".footerslide").click(function() {
			formid = $(this).data("formid");
			formContents = $("#" + formid).serializeArray();
			$.ajax({
				url: '/utilities/_formsession.php',
				type: 'post',
				data: formContents,
				success: function(data) {
					if (data.status == "success") {
						location.href = "/join-now";
					} else {
						
					}
				}
			});
		});
		$(".popupapply").click(function(e) {
			e.preventDefault();
			msg = "";
			$(".errors").removeClass("errors");
			$("#leadedit .validate").each(function() {
				if ($(this).val() == "") {
					$(this).addClass("errors");
					msg = "Y";
				}
			});
			gensel = citsel = milsel = greensel = false;
			$("#leadedit .gender").each(function(){ if ($(this).prop("checked")) { gensel = true;	} });
			$("#leadedit .milisel").each(function(){ if ($(this).prop("checked")) { milsel = true;	} });
			$("#leadedit .citizen").each(function(){ if ($(this).prop("checked")) { citsel = true;  } });
			if ($("#leadedit #NoCitizen").prop("checked")) {
				if ($("#leadedit #greencheck2").prop("checked")) {
					$("#leadedit").html("Based on your entries, you are not eligible to join the Air Force Reserve.");
					$(".popupapply").hide();
					return false;
				}
				$("#leadedit .greencard").each(function(){ if ($(this).prop("checked")) { greensel = true;	} });
			} else {
				greensel = true;
			}
			
			if (!gensel) { $("#leadedit #form_gender").addClass("errors"); msg = "Y"; }
			if (!citsel) { $("#leadedit #form_citizen").addClass("errors"); msg = "Y"; }
			if (!milsel) { $("#leadedit #form_prior").addClass("errors"); msg = "Y"; }
			if (!greensel) { $("#leadedit #greenrow").addClass("errors"); msg = "Y"; }
			
			if (msg) {
				return false;
			}
			formContents = $("#leadedit").serializeArray();
			$.ajax({
				url: '/utilities/_sideform_process.php',
				type: 'post',
				data: formContents,
				success: function(data) {
					if (data.status == "success") {
						location.href = "/thank-you?appid=" + data.appid;
					} else {
						alert("A lead has already been submitted with that information today");
					}
					
				},
				error: function(msg) {
					
				}
			});
		});
		$("#leadedit .citizen").click(function() {
			if ($(this).val() == "N") {
				$("#leadedit #greenrow").show();
			} else {
				$("#leadedit #greenrow").hide();
			}
		});
		$.ajax({
			url: "/load_social.php",
			type: 'get',
			data: {},
			success: function(data) {
				var q = $.parseJSON(data);
				for(var x=0; x < q.length;x++) {
					builder = linker = "";
					if (q[x]['link']) {
						linker = 'data-linktag="' + q[x]['link'] + '"';
					}
					builder += "<div id=\"scr" + x + "\" " + linker + " style=\"font-size: 12px;margin-bottom:15px;;background-color:#ffffff;color:#000000;padding:6px;text-align:left;\" class=\"feedclick " + q[x]['classer'] + "\">";
					builder += "<span class=\"squeezer\"><!-- " + q[x]['id_str'] + " -->";
					if (q[x]['picture']) {
						builder += "<center><img src=\"" + q[x]['picture'] + "\" style=\"max-width:248px;max-height: 110px;margin-right:10px;\"></center><br>";
					}
					builder += "<span class=\"contentfeeder\">";
					markey = q[x]['text'];
					markey2 = markey.replace(/\\/g,"");
					builder += Autolinker.link(markey2 ,{ newWindow: true }) + "</span></span><br>";
					builder += "<div style=\"clear:both\"><br></div>"
					builder += "<div style=\"color:#ffffff;border-radius:3px;background-color:" + q[x]['bgcolor'] + ";padding:5px;\">" + q[x]['from'] + " ";
					
					builder += " posted " + $.timeago(q[x]['created_at']) + "</div><br>";
					builder += "<div style=\"clear:both\"></div></div>"
					$("#socialdiv").append(builder);
					maxscroll++;
				}
				$("#socialdivloader").hide();
				$("#socialdiv").show();
				scrollem = setInterval(function() { scrollFeed(); },5000);
			},
			error: function(msg) {
				console.log(msg);
			}
		});
		$(".feedplaypause").click(function() {
			if ($(this).hasClass("fa-pause")) {
				pauseScroll();
				$(this).removeClass("fa-pause").addClass("fa-play")
			} else {
				scrollFeed();
				resumeScroll();
				$(this).removeClass("fa-pause").addClass("fa-pause")
			}
		});
	});
	</script>
