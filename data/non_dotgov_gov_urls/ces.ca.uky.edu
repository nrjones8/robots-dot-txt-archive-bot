  <!-- Move Bootstrap carousel caption below images -->
  <script>
    (function($) {
      $('.carousel-caption-below .carousel').carousel({pause: "false"});
      $('.carousel-play-pause').click(function () {
        if($(this).hasClass("fa-play")) {
          $(this).removeClass("fa-play");
          $(this).addClass("fa-pause");
          $(this).parents('.carousel-caption-below .carousel').carousel('cycle');
        }
        else {
          $(this).removeClass("fa-pause");
          $(this).addClass("fa-play");
          $(this).parents('.carousel-caption-below .carousel').carousel('pause');
        }
      });


      // Hide the default Bootstrap caption
      // and copy that html into the first caption-below span.
      $('.carousel-caption-below').each(function(){
        var caption = $(this).find('.carousel div.item:nth-child(1) .carousel-caption');
        $(this).find('.carousel-below span').html(caption.html());
        caption.css('display', 'none');
      });

      // This block cycles through to the next slide, hides the default Bootstrap
      // caption, and copies that html into the next caption-below span.
      // Working as expected.
      $(".carousel-caption-below .carousel").on('slide.bs.carousel', function(evt) {
        var caption = $(this).parent().find('div.item:nth-child(' + ($(evt.relatedTarget).index()+1) + ') .carousel-caption');
        $(this).siblings('.carousel-below').find('span').html(caption.html());
        caption.css('display','none');
      });
    })(jQuery);
  </script>

