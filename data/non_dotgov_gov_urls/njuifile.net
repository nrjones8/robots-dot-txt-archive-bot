    function messenger() {
      window.addEventListener('dfMessengerLoaded', function () {
        var messenger = document.querySelector('df-messenger');
        messenger.renderCustomText(`Welcome to the NJ Department of Labor's information virtual assistant, DOLi.  
         Please click here to ask your question.`)
        var chat = messenger.shadowRoot.querySelector('df-messenger-chat').shadowRoot.querySelector('df-message-list').shadowRoot.getElementById('messageList').firstChild.nextElementSibling;
        if (chat) {
          chat.innerHTML = `Welcome to the NJ Department of Labor's information virtual assistant, DOLi.  
             <br> Please <span style="text-decoration: underline"> click here </span> to ask your question.`
          chat.setAttribute('style', 'background-color: #4c5870; color: white')
        }
        messenger.showMinChat();
        let wrap = document
          .querySelector('df-messenger')
          .shadowRoot.querySelector('df-messenger-chat')
          .shadowRoot.querySelector('df-message-list')
          .shadowRoot.querySelector('div#messageList')
        wrap.addEventListener('mouseover', () => {
          messenger.setAttribute('intent', 'WELCOME')
        })
        wrap.addEventListener('mouseout', () => {
          messenger.removeAttribute('intent')
        })
        var closeIcon = document
          .querySelector('df-messenger')
          .shadowRoot.querySelector('df-messenger-chat')
          .shadowRoot.querySelector('df-message-list')
          .shadowRoot.querySelector('div')
          .querySelector('div#dismissIcon')
        closeIcon.addEventListener('click', () => {
          messenger.setAttribute('intent', 'WELCOME')
        })
      })
      let df = document.querySelector('df-messenger');
      df.addEventListener("df-request-sent", function (event) {
        var firstMessage = df.shadowRoot.querySelector('df-messenger-chat').shadowRoot.querySelector('df-message-list').shadowRoot.getElementById('messageList').firstChild.nextElementSibling;
        if (firstMessage) {
          firstMessage.setAttribute('style', 'background-color: white; color: black')
        }
      })
    }
