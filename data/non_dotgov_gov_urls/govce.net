<script type="text/javascript">

  function closedialog(){
    $a("#dialog").hide("drop", { direction: "down" }, 1000, callback);
  }

  function callback() {
    setTimeout(function() {
      $a("#dialog").removeAttr( "style" ).hide();
      }, 1000 );
  };
  
  $a(function () {
    $a("#dialog").dialog({
      autoOpen: false,
      width: 1000,
      height: 625,
      modal: true,
      position:"top",
      open: function(){
          // jms : close when clicking on background, outside of the dialog box.
          jQuery('.ui-widget-overlay').bind('click',function(){
              jQuery('#dialog').dialog('close');
          })
      },
      show: {
        effect: "puff",
        duration: 1000
      },
      hide: {
        effect: "explode",
        duration: 1000
      }
    });

    $a("#opener").click(function () {
      $a("#dialog").dialog("open");
    });

  });

  $a(document).ready(function () {
    $a(function () {
      $a("#tabs").tabs();
    });
  });

  $a(function () {
    $a("#accordion").accordion({
      collapsible: true,
      heightStyle: "content",
      active: false
    });
  });
  
      var messageDelay = 5000;
    var messageDelayForm = 5000;
    $a(init);

    function init() {
        $a('#contactForm').submit(submitForm);
        $a('#senderName').focus();
    }

    function submitForm() {
        var contactForm = $a(this);

        if (!$a('#senderName').val() || !$a('#senderEmail').val() || !$a('#message').val()) {
            $a('#incompleteMessage').fadeIn().delay(messageDelay).fadeOut();
            contactForm.fadeOut().delay(messageDelayForm).fadeIn();
        } else {
            //$a('#sendingMessage').fadeIn();
            //contactForm.fadeOut();

            $a.ajax({
                url: contactForm.attr('action') + "?ajax=true",
                type: contactForm.attr('method'),
                data: contactForm.serialize(),
            });

            submitFinished('success');
        }
        return false;
    }

    function submitFinished(response) {
        response = $a.trim(response);
        $a('#sendingMessage').fadeOut();

        if (response == "success") {
            $a('#senderName').val("");
            $a('#senderEmail').val("");
            $a('#message').val("");
            $a('#successMessage').fadeIn().delay(messageDelay).fadeOut();
            //$a("#contactForm").css({ opacity: 0.5 }).delay(messageDelayForm).css({ opacity: 1.0 });
            $a('#senderName').focus();
            //$a('#contactForm').fadeIn('slow', function () {
            //    $a('#senderName').focus();
            //})
        } else {
            $a('#failureMessage').fadeIn().delay(messageDelay).fadeOut();
            $a('#contactForm').delay(messageDelay + 500).fadeIn();
        }
    }
</script>

