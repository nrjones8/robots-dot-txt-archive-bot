<script type="text/javascript">
  	jQuery(document).ready(function() {
  		// Load dialog on click
		jQuery('.simpleModal').click(function (e) {
			var hrefValue = $(this).attr("href");
			var linkValue = hrefValue.replace('http://','');
			linkValue = linkValue.replace('www.','');
			linkValue = linkValue.replace('/find','');
			linkValue = linkValue.replace('/refuges/','');
			jQuery('#modal-content').modal({
				close: true,
				//escClose: true,
				//overlayClose: true,
				closeHTML: "<div>Close to go back<img src='./images/modal-close.png' width='30' height='30' style='vertical-align:middle;margin-left:10px;'></div>",
				closeClass: "modalClose"
			});
			// add the href to the button's anchor tag and bind the click of the link to close the modal
			jQuery('#modal-content a.button-link').attr("href", hrefValue).attr("target", "_blank").on( "click", function() {
				$.modal.close();
			});
			// add the web address to the button's text
			jQuery(".modal-continue-text-bottom").html('visit '+linkValue);
			return false;
		});
        var loc = window.location.pathname;
        jQuery("a[href='"+loc+"']").parent().addClass("active");
        jQuery("#fourth_grade_submit").click(function(event){
            event.preventDefault();
            if(jQuery("#fourth_grade_check").prop("checked") != true){
                alert("Please check the box to confirm you are a fourth grader.");
            }
            else {
                window.location.href="./start_game.htm";
            }
        });
    });
</script>
