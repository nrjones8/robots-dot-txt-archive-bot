				  mylat=isNaN(parseFloat(f.lat_dd.value))?0:parseFloat(f.lat_dd.value);
				  mylon=isNaN(parseFloat(f.long_dd.value))?0:parseFloat(f.long_dd.value);
				  
				  if (mylat<0 )
				  {
					f.lat_dir[1].checked=true;
					f.lat_dd.value=f.lat_dd.value*(-1)
				  } else {
