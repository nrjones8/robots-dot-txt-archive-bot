function CalculateCoord(){
	//console.log("in CalculateCoord");
	tobemapped=true;
	var f = document.enc_rec
        var m = 1.0/60.0
        var s = 1.0/3600.0
        var half_s = 0.5/3600.0
        var dd
        var mm
        var ss
        var t

	mylat=0
	mylon=0
