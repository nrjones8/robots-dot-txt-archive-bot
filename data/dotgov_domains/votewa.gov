        function SetAutoComplete() {
            $('#txtResAddress').autocomplete({
                source: function (request, response) {
                    $('#txtResAddress').addClass('loadinggif');
                    console.log('before ajax');
                    $.ajax({
                        url: "/TotalAddressAjax.aspx?value=" + request.term,
                        dataType: "json",
                        type: "GET",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            console.log('success ' + request.term);
                            if (data.length == 0) {
                                console.log('no data');
                                response(["no matching addresses found"]);
                                $('#txtResAddress').removeClass('loadinggif');
                            }
                            else {
                                console.log('got data');
                                response($.map(data, function (value, key) {
                                    return {
                                        label: value.display,
                                        value: value
                                    }
                                }));
                                console.log(data);
                                $('#txtResAddress').removeClass('loadinggif');
                            }
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            console.log(errorThrown);
                            //alert('Total Address is currently unavailable or having issues. Please try again later.');
                            $('#txtResAddress').removeClass('loadinggif');
                        }
                    });
                    console.log('after ajax');
                },
                select: function (event, ui) {
                    console.log('selected');
                    var add = ui.item.value;

                    console.log(add);
                    if (add == 'no matching addresses found') {
                        $('#txtResAddress').val(document.getElementById('txtResAddress').defaultValue);
                        return false;
                    }
                    $('#hdnPrecinct').val(add.precinctID);
                    $('#hdnAddress').val(add.display.replace(" " + add.zip, "").replace(" " + add.city, ""));
                    $('#hdnCity').val(add.city);
                    $('#hdnState').val(add.state);
                    $('#hdnZip').val(add.zip);
                    $('#txtResAddress').val($('#hdnAddress').val());
                    $('#btnSearchAddress').click();
                    return false;
                }
            });
        }

