@media 
only screen and (max-width: 1068px),
(min-device-width: 768px) and (max-device-width: 1068px)  {

    
	/* Force table to not be like tables anymore */
	table, thead, tbody, th, td, tr { 
		display: block; 
		border-collapse: separate;
		cellspacing: 0;
		cellpadding: 0;
		border-spacing: 0;
		padding: 0;
	}
	
	/* Hide table headers (but not display: none;  for accessibility) */
	thead tr { 
		position: absolute;
		top: -9999px;
		left: -9999px;
	}


	.HomeBody1, .HomeBody2, .HomeBody3 {
                padding: 0px !important;
	}

	
	td { 
		/* Behave  like a "row" */
		border-collapse: collapse;
		position: relative !important;
                width: 100%;
                height: auto;
                padding: 0px !important;
                margin: 0px !important;
	}

	.HomeBody1 td, .HomeBody2 td, .HomeBody3 td {
                width: 100%; 
                height: 450px;
                border-spacing: 15px 15px !important;
                margin: 0px !important;
	}
	
	td:before { 
		/* Now like a table header */
		position: absolute;
		/* Top/left values mimic padding */
		top: 6px;			
		white-space: nowrap;
	}

	table td.AboutUs:hover {
		border-collapse: collapse;
		background: linear-gradient(rgba(217, 217, 217, 0.9), rgba(217, 217, 217, 0.9)), url("images/hands.jpg");
