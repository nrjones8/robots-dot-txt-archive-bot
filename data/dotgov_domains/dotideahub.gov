<script>
function html_entity_decode(str)
{
	try
	{
		var tarea=document.createElement('textarea');
		tarea.innerHTML = str; return tarea.value;
		tarea.parentNode.removeChild(tarea);

	}
	catch(e)
	{
		//for IE add <div id="htmlconverter" style="display:none;"></div> to the page
		document.getElementById("htmlconverter").innerHTML = '<textarea id="innerConverter">' + str + '</textarea>';
		var content = document.getElementById("innerConverter").value;
		document.getElementById("htmlconverter").innerHTML = "";
		return content;
	}
}

function initPage()
{
	// When the user is not known to SiteMinder, it redirects back to
	// this page but with realmoid and type not set. Upon entering user
	// name again the browser displays an internal error. 
	//
	// Check for missing params and redirect to another errror page.
	if( '$$smusrmsg$$' == "" && '$$realmoid$$' == "" )
	{
		window.location="shimunknownuser.fcc";
	}
	var reject1 = "no";
	if (reject1=="yes")
	{
		var sp = document.getElementById("spNoMatch"); 
		sp.style.display = "";

		var PATH = "/";
		var DOMAIN = ".faa.gov";
		var ck = document.cookie.toLowerCase();
		if (ck.indexOf("reject=yes") >= 0) 
		{
			sp.style.display="";
			// expire the smtext cookie
			var now = new Date();
			now.setSeconds(now.getSeconds() - 1000);
			document.cookie = "REJECT=no;path=" + PATH + ";domain=" + DOMAIN + ";expires="+now;

		}
	}
	// Set focus
    //setfocus('username');
	
  	$("#userEmail").keyup( function() {
  		checkSubmitable();
	});
}

function checkSubmitable() {
	if($("#userEmail").val() != "") {
  		$("#cont").prop('disabled',false).removeClass("disabled");
 	} else {
 		$("#cont").prop('disabled',true).addClass("disabled");
	}
}

function usernameOnKeyDown() {
	if (event.keyCode == 13) { document.getElementById('userEmail').blur(); submitFormKey(); }
}

var Cnt;
function isValidEmailAddress(emailAddress) {
        var pattern = new RegExp(/^[+a-zA-Z0-9.'_-]+@[a-zA-Z0-9.-]+\.[gG][oO][vV]$/);
        return pattern.test(emailAddress);
    };
var valret;
function ValidateForm(userName,password) {
        valret = true;
        if (valret && (userName == "")) ShowErrorMessage("Enter Email");
        <!--if (valret && !(isValidEmailAddress(userName)))-->
			<!--ShowErrorMessage("Invalid Email - email should be an official government issued email (ending in .gov)");-->
        return valret;
    }

function ShowErrorMessage(msg, fromKey) {
    var userField = document.forms[0].USER;
    $('#dialog-error')
      .html(msg)
      .dialog({
        modal: true,
        open: function() {
          this.setAttribute('role','alert');
          this.setAttribute('tabIndex','-1');
          this.focus();
        },
        close: function() {
          userField.focus();
        },
        buttons: {
          OK: function() {
            if (Cnt == 1) {
              $(this).dialog("destroy");
              userField.focus();
            }
            else {
              Cnt--;
            }
            return true;
          }
        }
      });
    valret = false;
  }

function submitFormButton()
{
	Cnt=1;
	return submitForm();
}
function submitFormKey()
{
	Cnt=2;
	return submitForm();
}
function submitForm()
{
	var un = document.Login.USER.value;
	if(!ValidateForm(un,"") ){
	return false;
	}
	document.Login.action = "https://auth.smext.faa.gov/dotrso/arcotlogin/shim2.fcc";
	document.Login.submit();
}
function pivsubmitForm()
{	
	document.Login.action = "https://auth.smext.faa.gov/arcotafm/registration/pivredirect.jsp";
	document.Login.submit();
}
function DisplayMessage()
{
	var usrmsg = '$$smusrmsg$$';
	if (usrmsg !== null && usrmsg !== "")
	{
		var decodedHTMLStr = html_entity_decode('$$smusrmsg$$');
		var j = eval('(' + decodedHTMLStr + ')');
		if(j !== "" )
		{
			if( j.msg !== "" ){
				// no need to show error code
				// one can use switch to display user corresponding error msg
				// else display a standard message
				switch(j.code){
					case 706021001:
						document.write("<b>" + "Unable to create Token" + "</b><p>");
					break;
					case 706021002:
						document.write("<b>" + "Unable to retrieve Token" + "</b><p>");
					break;
					case 706021003:
						document.write("<b>" + "Unable to update Token" + "</b><p>");
					break;
					case 706021004:
						document.write("<b>" + "Unable to delete Token" + "</b><p>");
					break;
					case 706021005:
						document.write("<b>" + "Configured auth landling url does not match auth url in token. Contact Administrator." + "</b><p>");
					break;
					case 706021006:
						document.write("<b>" + "Unrecognized Shim Action. Contact Administrator." + "</b><p>");
					break;
					case 706021007:
						document.write("<b>" + "Initialization failed for the section. Contact Administrator." + "</b><p>");
					break;
					case 706021008:
						document.write("<b>" + "Password service not configured properly. Contact Administrator." + "</b><p>");
					break;
					case 706051102:
						document.write("<b>" + "Password field not present. Please retry." + "</b><p>");
					break;
					case 706051103:
						document.write("<b>" + "Invalid password format. Please retry." + "</b><p>");
					break;
					case 706051104:
						document.write("<b>" + "Token userID does not match disambiguated userID. Please retry." + "</b><p>");
					break;
					case 706051105:
						document.write("<b>" + "Backing scheme failure. Please retry." + "</b><p>");
					break;
					case 706051106:
						document.write("<b>" + "Risk Engine Post Eval denied transaction" + "</b><p>");
					break;
					case 706051107:
						document.write("<b>" + "Password reset failed. Please retry." + "</b><p>");
					break;
					case 706053201:
						document.write("<b>" + "User is disabled by administrator. Contact administrator." + "</b><p>");
					break;
					case 706053202:
						document.write("<b>" + "Max login attempts exceeded." + "</b><p>");
					break;
					case 706053203:
						document.write("<b>" + "User is inactive." + "</b><p>");
					break;
					case 706053204:
						document.write("<b>" + "Password expired." + "</b><p>");
					break;
					case 706053205:
						document.write("<b>" + "User is disabled in the directory." + "</b><p>");
					break;
					default :
						if(j.msg == "ShowStdErrMsg"){
							document.write("<b>" + "Invalid credential. Please retry.." + "</b><p>");
						}
						else
						{
							// these message will be any way localized by the AFM
							document.write("<b>" + j.msg + "</b><p>");
						}
				}//switch
			} // if( j.msg !== "" )
		} // if(j !== "" )
	}
}

