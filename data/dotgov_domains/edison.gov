<script type="text/javascript"> 
if(typeof jQuery!='undefined') {
	$(function() {
		var sectionMap = { //button key, and matched urls, order is important, keys using naming convention matching markup (below): "#nav_key"
				
				"idrSearch": ["=idrSearch", "SearchIDR" , "/invention/"], 
				"patentSearch": ["=patentSearch", "patent/search.era", "patent/record.era"],
				"utilSearch": ["=utilSearch", "utilization"], 
				
				"docSearch": ["=docSearch", "SearchDoc"], 
				"help": ["help"],
				"about": ["about"],
				"logout": ["logout"]
		}
		var l = window.location.href;
		var current = "";
		
		$.each(sectionMap, function(k,v) {
			$.each(v, function(i, item) {
				if(l.toLowerCase().indexOf(item.toLowerCase())!=-1)	{
					current = k;
					return false;
				}
			});
			if(current!="")	{
				return false;
			}	
		}); 
		
		//catch all
		if($("#mainMenuHeading").length)	{ 
			current = "mainMenu";
		}

		if(current)	{
			$('#nav_'+current).closest("li").addClass("activeSection");
		}
	});
}
</script>


