<script>
    $(window).load(function () {
        normalizeHeight = function (containerClass, siblingClass) {
            var $container = $(containerClass),
                $siblings,
                containerHeight = $container.outerHeight();

            if (siblingClass) {
                $siblings = $(siblingClass);
            } else {
                $siblings = $container.children();
            }

            var maxHeight = 0;
            $siblings.each(function () {
                var $this = $(this),
                    height = $this.outerHeight();
                if (height > maxHeight) {
                    maxHeight = height;
                }
            });
            $siblings.css('height', maxHeight)
            return maxHeight;
        }
        normalizeHeight(".boxheight", ".slidebox");
        normalizeHeight(".bxslider", ".bxslider img");
        $('.bxslider').bxSlider({
            mode: 'fade',
            auto: true,
            preloadImages: 'all',
            captions: true
        });
    });
</script>


