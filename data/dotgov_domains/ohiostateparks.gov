        <script>
        (function () {
            // Constants - Classes
            var NO_BACK_ICON = 'no-back-icon';
            var NO_CLICK = 'no-click';
            var BACK = 'back';
            var NEXT = 'next';
            var HOME = 'Home';
            var TRUE_STR = "true";
            var TAG_NAME = "tagName";
            var UL_STR = "UL";
            var LI_STR = "li"
            var HREF_STR = "href";
            var A_STR = "A";

            // Selectors
            var mainDivSelector = '#mobile-nav-menu';
            var mainDivListSelector = mainDivSelector + ' ' + LI_STR;
            var mainDivAnchorSelector = mainDivSelector + ' ' + A_STR;
            var mobileMenuElementSelector = '#mobile-menu';
            var navigationItem = 'li:not(.back)';
            var navigationItemSelector = mainDivSelector + ' ' + navigationItem;
            var navigationBackItem = 'li.back';
            var navigationBackItemSelector = mainDivSelector + ' ' + navigationBackItem;
            var pageLocationMetaDataElementSelector = '.sitepath-current-library-location';
            var hiddenSiteAreaSelector = 'li[site-area-hidden="Yes"]';

            // Constants - Characters
            var FSLASH = '/';
            var URLSLASH = '%2F';

            // Constants - IOP Library/Component Specific
            var URI_OHIO_CONTENT_ENGLISH = 'Ohio%2BContent%2BEnglish';
            var URI_PLUS_OHIO_CONTENT_ENGLISH = 'Ohio+Content+English';
            var URI_WCM_PATH_PREFIX = '?1dmy&urile=wcm%3apath%3a/';
            var NAVIGATOR_COMPONENT_LOCATION = '/wps/wcm/connect/gov/odx%20common%20design/utility/context' + '?source=library&srv=cmpnt&cmpntid=b62b20b5-d032-41f6-b997-3034ee3c9694';
            var landingPageWithCurrentSiteAreasLayout = 'layout-odx-single-column-with-bottom-sidebar-and-left-nav-parent-context';
            var queryKey = 'odnr-mobile-nav-start';

            // Constants - Data Attributes Key
            var attrDataSitePath = 'data-site-path';
            var attrDataValue = 'data-value';
            var attrDataLayout = 'data-layout';
            var attrDataParentTitle = 'data-parent-title';
            var attrAriaExpanded = 'aria-expanded';

            // State Variables
            var layout = '';
            var currentLibraryLocation = '';
            var sitemapCache = {};
            var movement = [];

            // Helpers - URL Utils
            var getURLParts = function (url, splitBy) {
                if (url === undefined) {
                    return [''];
                } else {
                    return url.split(splitBy)
                        .filter(function (item) { return item; });
                }
            }

            var avoidMovingBack = function () {
                var backLocation = getURLParts($(navigationItemSelector)
                    .first()
                    .attr(attrDataSitePath)
                    , FSLASH);

                if (backLocation.length <= 3) {
                    return true;
                }
                return false;
            }

            // Get path in library where we need to open the menu in context
            var getLocationToMoveInContext = function () {
                var locationFinder = [FSLASH + URI_PLUS_OHIO_CONTENT_ENGLISH]
                    .concat(
                        getURLParts(
                            getURLParts(currentLibraryLocation, URI_OHIO_CONTENT_ENGLISH)[1],
                            URLSLASH)
                    );
                locationFinder.pop();
                if (layout !== landingPageWithCurrentSiteAreasLayout) {
                    locationFinder.pop();
                }
                return locationFinder.join(FSLASH);
            }

            // Apply Home page Navigation updates when the user ends in the First Level 
            var applyBackButtonMarkup = function () {
                var backLocation = getURLParts($(navigationItemSelector)
                    .first()
                    .attr(attrDataSitePath)
                    , FSLASH);

                if (backLocation.length <= 3) { // We are in home page level
                    [NO_BACK_ICON, NO_CLICK].map(function (cls) { $(navigationBackItemSelector).addClass(cls) });
                    $(navigationBackItemSelector).html(HOME);
                } else {
                    [NO_BACK_ICON, NO_CLICK].map(function (cls) { $(navigationBackItemSelector).removeClass(cls) });
                    // Update the top < {BACK} title to the parent site area
                    $(navigationBackItemSelector)
                        .html(
                            $(navigationItemSelector)
                                .first()
                                .attr(attrDataParentTitle)
                        );
                }
            }

            // Navigator Service URL
            var getServiceURL = function (sitePath) {
                var serviceURL = window.location.origin +
                    NAVIGATOR_COMPONENT_LOCATION;
                serviceURL = serviceURL + '&' + queryKey + '=' + sitePath +
                    (window.debug_MobNavCacheOff ? '&' + Math.random() + '=' + Math.random() : '');
                return serviceURL;
            }

            var bindEvents = function () {
                $(mainDivListSelector).click(function (e) {
                    // If User clicks on anchor then allow the user to go to the page and stop reveal
                    if (e.target.nodeName === A_STR) {
                        return;
                    }

                    if ($(this).hasClass(NO_CLICK) && $(this).hasClass(BACK)) {
                        return;
                    }

                    if ($(this).attr(attrDataSitePath)) {
                        move($(this).attr(attrDataSitePath));
                    } else {
                        moveBack();
                    }
                });
            }

            var moveBack = function (skipInsertToDOM) {
                var backLocation = getURLParts(
                    $(navigationItemSelector)
                        .first()
                        .attr(attrDataSitePath),
                    FSLASH
                );
                backLocation.pop();
                backLocation.pop();
                backLocation = FSLASH + backLocation.join(FSLASH);
                move(backLocation, skipInsertToDOM);
            }

            // For the Anchors
            var mapURL = function (url) {
                return URI_WCM_PATH_PREFIX +
                    URI_PLUS_OHIO_CONTENT_ENGLISH +
                    getURLParts(url, URI_PLUS_OHIO_CONTENT_ENGLISH)[1];
            }

            // Add Split Nav + Disallow/Allow Click
            var updateUINavigation = function () {
                $(mainDivSelector + ' ' + navigationItem).each(function () {

                    // ODNR SPECIFIC , No landing page in level 2,3
                    if ($(this).attr(attrDataSitePath)) {
                        var sitePathParts = getURLParts($(this)
                            .attr(attrDataSitePath), FSLASH);

                        if (sitePathParts.length <= 4) {
                            $(this).addClass(NO_CLICK);
                        }
                    }
                    // END ODNR SPECIFIC

                    if ($(this).next().prop(TAG_NAME) === UL_STR) {
                        // Find if there are site area inside a site area that are actually visible
                        if ($(this).next().children(navigationItem).length !== $(this).next().children(hiddenSiteAreaSelector).length) {
                            $(this).addClass(NEXT); // Allow User to reveal + Add icon for Chevron Right
                        }
                    }
                });
            }

            // Insert the response to DOM
            var insertToDOM = function (data) {
                if (data.length > 0) {
                jQuery(mainDivSelector).empty();
                //INSERT to DOM

                
                jQuery(mainDivSelector)
                    .html(replaceContentLinksToPortalLinks(data));
}
                // Update DOM Anchor links
                /*jQuery(mainDivAnchorSelector).each(function () {
                    $(this).attr(HREF_STR,
                        mapURL($(this).attr(HREF_STR))
                    )
                });*/
                updateUINavigation();
                bindEvents();
                applyBackButtonMarkup();
            }

            // Move to the next Menu Items
            var move = function (path, skipInsertToDOM) {
                // movement.push(path); console.log(movement);
                // RENDER FROM CACHE
                var cacheKey = btoa(path);
                if (!window.debug_MobNavCacheOff && sitemapCache[cacheKey]) {
                    if (!skipInsertToDOM) {
                        insertToDOM(sitemapCache[cacheKey]);
                    }
                    return;
                }
                // END RENDER FROM CACHE

                $.ajax({
                    url: getServiceURL(path),
                })
                    .done(function (data) {
                        sitemapCache[btoa(path)] = data;

                        if(window.odnrCapMobDump){console.log(JSON.stringify(sitemapCache));}
                        if (data.length === 0) {
                           window.location = window.location.origin + path.replace(/\/ohio\+content\+english|\/Ohio\+Content\+English/g, '/wps/portal/gov');
                        }
                        if (!skipInsertToDOM) {
                            insertToDOM(data);
                        }
                    });
            }

            var setCurrentLocation = function () {
                // Get the Current Page for opening the Nav in context every time user hits the mobile menu
                $(pageLocationMetaDataElementSelector).each(function () {
                    if ($(this).attr(attrDataValue).indexOf(URI_OHIO_CONTENT_ENGLISH) > -1) {
                        currentLibraryLocation = $(this).attr(attrDataValue);
                        layout = $(this).attr(attrDataLayout);
                    }
                });
            }

            // START INIT Scripts
            //
            //

            // Render Menu in current context
            $(document).ready(function () {
                // Event Listener for Mobile Menu Click and initiate Render Menu in current context
                $(mobileMenuElementSelector).click(function () {
                    if ($(this).attr(attrAriaExpanded) === TRUE_STR) {
                        $(mainDivSelector).show();
                    }
                    move(getLocationToMoveInContext());
                });
                // INIT Main
                setCurrentLocation();
                // move(getLocationToMoveInContext(), true);
            });

            //
            //
            // END INIT SCRIPTS
        })();
    </script>
</div>

