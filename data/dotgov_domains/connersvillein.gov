<script>

    $(function () {
        var ViewportHeight = $(window).height();
        var Pinned = false;
        var DashboardHeight = $('.CSK_Dashboard').height();
        var TopOffset = (ViewportHeight / 2) - (DashboardHeight / 2);
        $('.CSK_Dashboard').css("top", TopOffset);
        $('.CSK_Dashboard_Handle').css("top", (DashboardHeight / 2) - 20);

        $('.CSK_Dashboard h1 a').click(function () {
            if (Pinned)
            {
                $('.CSK_Dashboard h1 a').css('backgroundPosition', 'right 2px');
                Pinned = false;
            }
            else
            {
                $('.CSK_Dashboard h1 a').css('backgroundPosition', 'left top');
                Pinned = true;
            }


        });

        $('.CSK_Dashboard').hover(
            function () {
                $('.CSK_Dashboard').stop().animate({ right: 0 }, 300);
                $('.CSK_Dashboard_Handle').animate({ width: 0, left: 0 }, 300);
                $('.CSK_Dashboard_Handle').css('borderLeftWidth', '0');
                $('.CSK_Dashboard_Handle').css('borderRightWidth', '0');

            },
            function () {

                if (!Pinned) {
                    $('.CSK_Dashboard').stop().animate({ right: -300 }, 300);
                    $('.CSK_Dashboard_Handle').stop().animate({ width: 40, left: -42 }, 300);
                    $('.CSK_Dashboard_Handle').css('borderLeftWidth', 1);
                    $('.CSK_Dashboard_Handle').css('borderRightWidth', 1);
                }
            }
        );


    });

    //$('#CSK_Dashboard')

</script>

<style>
    .CSK_Dashboard {
        position: fixed;
        width: 276px;
        z-index: 20000000;
        right: -300px;
        background: #f1f1f1;
        border: 1px solid #d1d1d1;
        border-left: 1px solid #d1d1d1;
        border-right: medium none;
        padding: 12px;
    }

        .CSK_Dashboard h1 {
            border-bottom: 1px dotted #D1D1D1;
            font-size: 22px;
            line-height: normal;
            margin: 0 0 6px;
            padding: 0 0 3px;
        }

            .CSK_Dashboard h1 a {
                background-image: url(data:image/png;base64,);
                background-position: right 2px;
                display: block;
                float: right;
                height: 16px;
                margin-top: 6px;
                overflow: hidden;
                width: 16px;
                outline:none!important;
            }

        .CSK_Dashboard h2 {
            font-size: 16px;
            line-height: normal;
            margin: 0 0 3px;
            padding: 0;
        }

    .CSK_Dashboard_Handle {
        position: absolute;
        height: 40px;
        width: 40px;
        left: -42px;
        background: #f1f1f1;
        border: 1px solid #d1d1d1;
        border-right: 1px solid #d1d1d1;
        background-image: url(data:image/png;base64,);
        background-position: center;
        background-repeat: no-repeat;
    }

    .CSK_Dashboard ul {
        margin: 0 0 12px;
        padding: 0;
    }

        .CSK_Dashboard ul li {
            list-style-type: none;
            line-height: 1.4em;
            font-weight: bold;
        }

            .CSK_Dashboard ul li span {
                font-weight: normal;
            }

            .CSK_Dashboard ul li a, .CSK_Dashboard ul li a:link, .CSK_Dashboard ul li a:hover, .CSK_Dashboard ul li a:visited, .CSK_Dashboard ul li a:active {
                color: #333;
            }
</style>

 

