<div ng-controller="facrbookCtrl">
    <img id="mySpinner" src="/Project/Contents/Main/_gfx/cmn/loading.gif" ng-show="loading" style="margin: 0 auto; display:block;" />

    <div class="facebook_content ng-hide" ng-hide="loading">
        <ul class="clearfix">
            <li ng-repeat="f in feeds">
        <a href="{{getLink(f)}}" target="_blank">
                    <img ng-if="f.picture" src="{{f.picture}}" alt="" />
                    <img ng-if="f.picture == null" src="{{logo_image}}" alt="" />
                </a>
        <span class="time_date">{{f.updated_time | date:"MMMM dd, yyyy hh:mm a"}}</span>
                <a href="{{getLink(f)}}" target="_blank">
                    {{getDescription(f) | limitTo: 200}}{{getDescription(f).length > 200 ? '...' : ''}}
                </a>
            </li>
        </ul>
    </div>
    <a ng-hide="loading" href="{{info.link}}" target="_blank" class="box_bottomlink  ng-hide">More Posts</a>

</div>

<script>


    (function (window, angular) {
        'use strict';

        window.viCurrentModule.requires.push("facebookApp");
       
        angular.module('facebookApp', [])
            .controller('facrbookCtrl', function($scope, $http) {
                /*
                $scope.Account = "CityofGillette";
                $scope.feeds = "";
                $scope.info = "";
                $scope.logo_image = "//graph.facebook.com/" + $scope.Account + "/picture?access_token=703993119616427%7Cd0cb805fe222919e0702f32d535401d7&type=normal";
                */
                
                $scope.Account = "CityofGillette";
                $scope.feeds = [];
                $scope.info = "";

                $scope.count = 2;
                $scope.type = "posts"; //feed or posts

				var infoUrl =  "/Sys/Handler/facebookfeed" + "?account=" + $scope.Account + "&type=info";
				$scope.loadMoreUrl = "/Sys/Handler/facebookfeed" + "?account=" + $scope.Account + "&type=" + $scope.type + "&limit=" + $scope.count;
				$scope.profile_image = "//graph.facebook.com/" + $scope.Account + "/picture?type=square";
				$scope.logo_image = "//graph.facebook.com/" + $scope.Account + "/picture?type=normal";                
                

                // Show loading spinner.
                $scope.loading = true;

                $scope.showPhoto = function(item) {
                    if (item.type == 'photo') {
                        return true;
                    }
                    return false;
                }

                $scope.showLink = function(item) {
                    if (item.type == 'link') {
                        return true;
                    }
                    return false;
                }

                $scope.showStatus = function(item) {
                    if (item.type == 'status') {
                        return true;
                    }
                    return false;
                }

                $scope.showVideo = function(item) {
                    if (item.type == 'video') {
                        return true;
                    }
                    return false;
                }

                $scope.getLink = function(item) {
                    if (item.type == 'status') {
                        return "//www.facebook.com/" + $scope.Account + "/posts/" + item.id.replace(item.from.id + "_", "");
                    } else {
                        return item.link;
                    }
                }

                $scope.getTitle = function(item) {
                    if (item.name) {
                        return item.name;
                    } else {
                        return item.from.name;
                    }
                }

                $scope.getDescription = function(item) {
                    if (item.message) {
                        return item.message;
                    } else if (item.description) {
                        return item.description;
                    } else {
                        return item.from.name;
                    }
                }

                $scope.getImage = function(item) {

                    if (item.picture) {
                        return item.picture;
                    } else {
                        return "";
                    }

                }

                $http.get(
                    infoUrl
                )
                .then (function onSuccess(response) {
                    $scope.info = response.data;

                });

                $http.get(
                        $scope.loadMoreUrl
                    )
                    .then (function onSuccess(response) {
                        $scope.feeds = response.data.posts.data;

                    })
                    .finally(function () {
                    // Hide loading spinner whether our call succeeded or failed.
                    $scope.loading = false;
                });

                

            });

    })(window, angular);

    </script>
                
