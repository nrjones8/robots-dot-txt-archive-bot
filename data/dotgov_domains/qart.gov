    <body class="login-body">
        <!-- Begin page content -->
        <div class="container" id="login-main">
            <div class="row" > 
                <div id="logo" >
                    <div class="row">
                        <img src="/dni/qart/resources/images/qart-logo-final.png" alt="ODNI Official Logo" />
                        <p>[kart]</p>
                    </div>
                </div><!-- End Div Logo -->
            </div><!-- End Div Row -->

            <div class="row" style="margin-top:-80px;">
                <div  id="long-name" >
                    <p>QUALITY ASSESSMENT REPORTING  TOOL</p>
                </div>
            </div>

            <div class="row" id="form-row" >
                
                

                <form id="user-login" class="form-horizontal" action="/dni/qart/login" method="post" role="form">
                        <div class="form-group username-formgroup">
                            <label for="user-name" class="col-sm-4 control-label">Username</label>
                            <div class="col-sm-7">
                                <input type="text" class="form-control col-sm-4" name="username" id="user-name" autofocus="autofocus"/>
                            </div>
                        </div><!-- End div username-formgroup -->

                        <div class="form-group">
                            <label for="password" class="col-sm-4 control-label">Password</label>
                            <div class="col-sm-7">
                                <input type="password" class="form-control" name="password" id="password" />                
                                <p class="text-center"><a href="/dni/qart/password-recovery">Forgot your password?</a></p>
                        </div>
                    </div><!-- End div password formgroup -->

                    <div class="form-row">
                        <div class="col-sm-12 text-justify">
                            <h4>Security Reminder</h4>
                            <p>Use of this U.S. Government system, authorized or unauthorized, constitutes consent to monitoring of this 
                                system. Unauthorized use may subject you to criminal prosecution. Evidence of unauthorized use collected 
                                during monitoring may be used in administrative, criminal, or other adverse actions.</p>

                        </div>
                    </div>
                    <div class="text-center" >
                        <button type="submit" class="btn btn-primary btn-lg" style="margin-bottom:20px;">Login</button>
                    </div> 
                    <input type="hidden" name="_csrf" value="b3a022fd-92bc-47d0-bc9c-21e869cd76f8" />
                </form>
            </div><!-- End div form-row-->
        </div><!-- End div login-main -->

        <div id="footer">
            <div class="container">
                <div class="row">
                    <div class="col-sm-5">
                        <p class="text-muted">This site is owned by the Office of the Director of National Intelligence
                            and can be used for the assessment of Public Trust and National Security Investigations.
                        </p>
                        <p class="text-muted">If you have questions regarding the assessment of background investigations
                            or the tool, please contact the QART Team at <a href="mailto:qart@dni.gov">qart@dni.gov</a>.
                        </p>
                    </div>
                    <div class="col-sm-3"></div>
                    <div class="col-sm-4">    
                        <a href="http://www.odni.gov/index.php"><img src="/dni/qart/resources/images/dni-small.png" id="odni-image" alt="ODNI seal" /></a>
                        <a href="http://www.opm.gov/"><img src="/dni/qart/resources/images/opm-small.png" id="opm-image" alt="OPM seal" /></a>
                    </div>
                </div><!-- End div row -->
            </div><!-- End div container -->
        </div><!-- End Div Footer -->

