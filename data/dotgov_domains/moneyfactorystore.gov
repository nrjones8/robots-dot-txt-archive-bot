<script>

(function($) {
	$(document).ready(function() {
		// ===================================================================
		// Bind all caro functions.
		// ===================================================================

		// CARO featured images
		// TODO: 
		// = add the ability to change arrow color
		$('.campaign-featured .js-caro-images').slick({
			accessibility: true,
			dots: false,
			infinite: true,
			speed: 300,
			autoplay : true,
			autoplaySpeed : 12000,
			slidesToShow: 1,
			prevArrow: '<div class="caro-arrow prv-arrow"></div>',
			nextArrow: '<div class="caro-arrow nxt-arrow"></div>'
		});

		// CARO Featured Section
		$('.featured-full-caro.js-caro-featured').closest('.featured-wrapper').append('<div class="slick-caro-controls"></div>')
		$('.js-caro-featured').slick({
			dots: true,
			appendDots: $('.slick-caro-controls'),
			speed: 1000,
			slidesToShow: 1,
			autoplay : false,
			autoplaySpeed : 12000,
			arrows: false,
			slidesToShow: 1,
			slidesToScroll: 1,
		});

		// pause and play button won't work unless autoplay is set to false 
		// above and this slickPlay function is called.
		$('.js-caro-featured').slick('slickPlay');

		$('.featured-wrapper .slick-caro-controls').append('<span class="caro-play">PLAY</span><span class="caro-stop">STOP</span><button class="slick-ctrl slick-pause playing" title="Stop">STOP</button>')

		// change dot and play button color based on data-carodot-color attribute.
		if($('.js-caro-featured').data('carodot-color') && $('.js-caro-featured').data('carodot-color') != '') {
			var dotColor = $('.js-caro-featured').data('carodot-color').toString()
				style = '<style>';
				style += '.featured-wrapper .slick-dots li button:before {background-color:'+dotColor+';}';
				style += '.featured-wrapper .slick-caro-controls button.slick-ctrl {color:'+dotColor+';}';
				style += '.featured-wrapper .slick-caro-controls span.caro-play {border-left-color:'+dotColor+';}';
				style += '.featured-wrapper .slick-caro-controls span.caro-stop {background-color:'+dotColor+';}';
				style += '</style>';
			$('.js-caro-featured').append(style)
		}

		// change text and control of carousel play and stop button
		$('.slick-ctrl').on('click', function() {

			var $pauseBtn = $(this),
				$caro = $pauseBtn.parents().find($('.js-caro-featured'));

			// if has "playing" class
			if ($pauseBtn.hasClass('playing')) {
				$pauseBtn.removeClass('playing')
					.addClass('paused')
					.text("Play")
					.attr('title', 'Play');
				$('.caro-play').show();
				$('.caro-stop').hide();
				$caro.slick('slickPause');
			} else {
				$pauseBtn.text("Stop");
				$pauseBtn.removeClass('paused')
					.addClass('playing')
					.text("Stop")
					.attr('title', 'Stop');
				$('.caro-play').hide();
				$('.caro-stop').show();
				$caro.slick('slickNext')
				$caro.slick('slickPlay');
			}
		});


		// CARO products
		$('.campaign-products.js-caro-products').slick({
			dots: false,
			infinite: true,
			speed: 300,
			slidesToShow: 4,
			slidesToScroll: 1,
			prevArrow: '<div class="caro-arrow-prod prv-arrow"></div>',
			nextArrow: '<div class="caro-arrow-prod nxt-arrow"></div>',
			responsive: [
				{
					breakpoint: 1023,
					settings: {
						slidesToShow: 3,
						slidesToScroll: 1,
						infinite: true,
					}
				},
				{
					breakpoint: 600,
					settings: {
						slidesToShow: 2,
						slidesToScroll: 1
					}
				},
				{
					breakpoint: 480,
					settings: {
						slidesToShow: 1,
						slidesToScroll: 1,
						adaptiveHeight: true
					}
				}
			]

		});
	});
}(jQuery))

</script>


