		$('#es-sms-alert-button-footer').on('click',do_footer_sms_signup);

		function do_footer_sms_signup() {

			$('#es-sms-alert-button-footer, #sms-signup-footer').prop('disabled',true);

			var url = "https://catalog.usmint.gov/on/demandware.store/Sites-USM-Site/default/EmailSignup-SMSOptinDirect?keyword=JOINFOOTER";
			var phone = $('#sms-signup-footer').val().replace(/[^0-9]/,'');

			var phone_re = /^1?[0-9]{10}$/;

			if (!phone_re.test(phone)) {
				alert('Please input a valid United States phone number.');
				$('#es-sms-alert-button-footer, #sms-signup-footer').prop('disabled',false);
				return;
			}

			var params = {
				'phone': phone
			}

			$.get(url,params,function(resp){
				$('#es-sms-alert-button-footer, #sms-signup-footer').prop('disabled',false);
				if (resp) {
					$('#sms-signup-footer').val('');
					alert('Thank you for signing up!');

	try {
		digitalData.event = {
