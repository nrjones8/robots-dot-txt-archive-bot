		    			jQuery('#ModalEmailSignupForm').addClass('error');
		        	}
		        }
	      	});
		
		    return false;
		});
	});		
	
	var cookies = document.cookie.split( ';' ).map( function( x ) { return x.trim().split( '=' ); } ).reduce( function( a, b ) { a[ b[ 0 ] ] = b[ 1 ]; return a; }, {} );

	function init_emailmodal() {
		var pageviews=0;
		
		// set some test values here from DW or wherever
		var cond1 = parseInt("3");		
		var cond2 = 'dont_show_email_modal'; // cookie name to skip showing
		
		if (cookies[cond2]) {
			return;
		}
		if (cookies.pageviews != undefined) {
			pageviews = (parseInt(cookies.pageviews)+1);
			document.cookie="pageviews="+pageviews+"; path=/";
		} else {
			pageviews = 1;
			document.cookie="pageviews=1; path=/";
		}
		
		$('#emailmodalclose').on('click',close_emailmodal);
		$('#emailmodalcloseconfirm').on('click',close_emailmodalconfirm);
		
		// put some conditional tests here
		if (pageviews < cond1) {
			return;
		}

		$('#emailmodaldialog-wrapper').show();
		
	}

	function close_emailmodal(e,dur) {
		//Get site pref that stores the days to set the 'dont show' cookie upon closing modal manually.		
		var exdays = 0;
		if (dur != undefined) {
			exdays = dur;
		} else {
			exdays = parseInt("60");
		}
		var d = new Date();
	    d.setTime(d.getTime() + (exdays*24*60*60*1000));
	    var expires = "expires="+d.toUTCString();
		document.cookie='dont_show_email_modal=1;'+expires+'; path=/';
		document.cookie="pageviews=; path=/";

		$('#emailmodaldialog-wrapper').remove(); /*remove it unless youre going to use it again in the same pageview, probably not.*/
	}
	function close_emailmodalconfirm() {
		$('#emailmodaldialog-confirm').remove(); /*remove it unless youre going to use it again in the same pageview, probably not*/
		
	}
	$('#ModalEmailSignupForm input[name$="firstname"]').attr('placeholder','First Name');
	$('#ModalEmailSignupForm input[name$="dwfrm_emailsignup_lastname"]').attr('placeholder','Last Name');
	$('#ModalEmailSignupForm input[name$="dwfrm_emailsignup_email"]').attr('placeholder','Email Address');
</script>
	




