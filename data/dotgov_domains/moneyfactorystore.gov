			}
			else {
				jQuery('#formAgreementContainer .erroragreement').show();
				return false;
			}
		    var url = "/on/demandware.store/Sites-USM-Site/default/EmailSignup-ModalSignupAjax";
		    jQuery.ajax({
		        type: "POST",
		        url: url,
		        data: jQuery('#ModalEmailSignupForm').serialize(),
		        dataType: "json",
		        success: function(data)
		        {
		        	if (data.success == "True") {
		        		close_emailmodal(null,730);
