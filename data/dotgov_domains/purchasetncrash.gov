<style>
    .collapsible {
        background-color: #f9f9f9;
        color: #1b365d;
        cursor: pointer;
        padding: 18px;
        width: 40%;
        border: none;
        text-align: left;
        outline: none;
        font-size: 15px;
    }
    
    .active,
    .collapsible:hover {
        background-color: none;
    }
    
    .collapsible:after {
        content: '\002B';
        font-weight: bold;
        float: left;
        margin-left: 5px;
    }
    
    .active:after {
        content: "\2212";
    }
    
    .content {
        padding: 0 18px;
        max-height: 0;
        overflow: hidden;
        transition: max-height 0.2s ease-out;
        background-color: inherit;
        ;
    }
    
    #mydiv {
        width: 100%;
        text-align: center;
        background-color: lightblue;
        margin-top: 0px;
    }
    /**************/
    /* Responsive */
    /**************/
    
    @media only screen and (max-width: 40em) {
        .collapsible {
            width: 100% !important;
        }
        select {
            width: 100% !important;
        }
    }
    
    @media (max-width: 400px) {
        #content .row .four,
        #content .row .eight {
            width: 100%;
        }
        #content .row {
            display: flex;
            flex-direction: column;
        }
        #content .row .four {
            order: 2;
        }
        #content .row .eight {
            order: 1;
            margin-top: 20px;
        }
    }
</style>
