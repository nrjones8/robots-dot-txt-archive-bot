	<script>
		$(document).ready(function () {
			// Global A11Y Fixes
			$('a[href^="https://www.elocallink.tv/m/v/player.php"]').attr("aria-label","Watch County Tour Video");
			$('a[href^="https://www.elocallink.tv/m/v/player.php"]').find("span").attr("aria-hidden","true");
			var i=1;
			$("h1, h2, h3, h4, h5, h6").each(function(){
				$(this).attr("role","heading");
				$(this).attr("aria-level",i);
				i++
			})
			$(".resp-accordion").each(function() {
				$(this).attr("role","button");
			})
			$(".resp-tab-content").each(function() {
				$(this).attr("role","tabpanel");
				$(this).attr("id",$(this).attr("aria-labelledby"));
			})
			$(".resp-arrow").each(function() {
				$(this).attr("role","button");
				$(this).attr("aria-label","Open Tab");
			}) 
		})	
	</script>
  
  
