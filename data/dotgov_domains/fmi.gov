    <script type="text/javascript">
		$(function() {
            //Project Menu -- To Backbone!
            window.SfTool.activeProjectId = false;

            $('#projectNav--Link').on('click', function(e) {
                if (!window.SfTool.activeProjectId) return; // We don't want to prevent the bubble if there isn't a menu.
                e.preventDefault();
                var $this = $(this),
                    $target = $($this.attr('data-target')),
                    width = $target.width();
                position = $this.position();
                $target.css('top', position.top + 12 + 'px')
                    .css('right', 0 + 'px');
                $target.toggle();
            });

            $("#projectNav").on("click", '.removeMaterialLink', function(e) {
                e.preventDefault();
                var $this = $(this),
                    href = $this.attr("href"),
                    materialId = $this.attr('data-material-id');
                //We know we are removing the material so isAdd is false
                //and we also know that we don't use the material name when we are removing...
                window.SfTool.MaterialAction(false, '', window.SfTool.activeProjectId, materialId, href);
            });

            //END Project Menu

            // has to be down here bc the <span> injection can mess up the existing javascript.
            var searchTerm = window.SfTool.getParameterByName('hw');
            if (typeof searchTerm === 'string') {
                // oh goodness this prefix is such a stupid hack
                // but just accept it.  seriously.  it harms nobody.
                searchTerm = "?h=" + searchTerm;
                window.SfTool.localSearchHighlight(searchTerm);
            };

            /*
            * Autocomplete
           */
                    
                        window.SfTool.initializeAutocomplete("#siteSearch input:text", "/content/json/autocomplete_sftool.json");
                    


            /*
             * Some stuff that was in sftool.js, but doesn't belong there because it's not library code.
             */
            $('.CRUDMsg').fadeOut(2000, function () {
                // Animation complete.
            });

            // input placeholders
            $('input').placeholder();
        });

