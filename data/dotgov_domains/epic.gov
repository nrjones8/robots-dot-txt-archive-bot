        </div>
    </div>
    <script type="text/javascript">
        window.addEventListener("load", resizeFunction);
        window.addEventListener("resize", resizeFunction);

        function resizeFunction() {
            var newHeight=1;
            var adjustedHeight=getComputedHeight("spaceForHeadingBar");
            adjustedHeight+=getComputedHeight("footerBar")+35;

            var newHeight=(window.innerHeight) - adjustedHeight;
            if (newHeight < 1){
                newHeight=1;
            }
            document.getElementById("fullHeight").style.minHeight=newHeight+'px';
        }

        function getComputedHeight(theElt){
            tmphght = document.getElementById(theElt).offsetHeight;
            return tmphght;
        }
    </script>
</body></html>