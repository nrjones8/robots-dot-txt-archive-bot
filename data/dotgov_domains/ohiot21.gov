        function getShortUrl() {
            var url = '?1dmy&urile=wcm%3apath%3a%2FOhio%2BContent%2BEnglish%2Fodh%2Fknow-our-programs%2Ftobacco-use-prevention-and-cessation%2FTobacco-21%2FTobacco-21';
            var tempArray = url.split('%2F');
            tempArray.splice(0, 2);
            var urlRightSide = tempArray.join('/');
            var urlLeftSide = window.location.protocol + '//' + window.location.hostname + '/wps/portal/' + window.portalContext + '/';
            return urlLeftSide + urlRightSide;
        }
        var agency = window.OHIO.ODX.configuration.currentAgency;
        var twitterProfileID = window.OHIO.ODX.configuration.API.Twitter["site"];
        var facebookConfig = window.OHIO.ODX.configuration.API.Facebook["site"];
        if ( typeof window.OHIO.ODX.configuration.API.Twitter[agency] != "undefined") {
            twitterProfileID = window.OHIO.ODX.configuration.API.Twitter[agency].profileID;
        }
        if ( typeof window.OHIO.ODX.configuration.API.Facebook[agency] != "undefined") {
            facebookConfig = window.OHIO.ODX.configuration.API.Facebook[agency];
        }
        $('.odx-share-twitter__anchor').click(function(e) {
            e.preventDefault();
            var shareWindow = window.open('https://twitter.com/intent/tweet?text=Tobacco 21&url=' + getShortUrl() + '&via='+ twitterProfileID, 'ShareWindow', 'height=445,width=520,resizable');
            if (window.focus) {
                shareWindow.focus()
            }
        });
        $('.odx-share-facebook__anchor').on('click', function (e) {
            e.preventDefault();
            var publicURL = getShortUrl();
            var articleTitle = 'Tobacco 21';
            publicURL = publicURL.replace('myportal', 'portal');
            articleTitle = $('<div/>').html(articleTitle).text();
            
            
            var articleDesc = 'This area of the website focuses on Ohio&#39;s Tobacco 21 law.';
            
            articleDesc = $('<div/>').html(articleDesc).text();
            
            
            var articleImg = '/wps/wcm/connect/gov/2b7c045e-94db-464b-92cc-0b005f88674f/Tobacco-21-1280x480.jpg?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE.Z18_M1HGGIK0N0JO00QO9DDDDM3000-2b7c045e-94db-464b-92cc-0b005f88674f-mWf5iHK';
            
            articleImg = articleImg.replace('myconnect', 'connect');
            articleImg = window.location.protocol + "//" + window.location.host + articleImg;
            FB.init({
                appId: facebookConfig.appId,  // '2052413878334763',
                xfbml: true,
                version: 'v2.12'
            });
            FB.ui({
                method: 'share_open_graph',
                action_type: 'og.shares',
                action_properties: JSON.stringify({
                    object: {
                    'og:url': publicURL,
                    'og:title': articleTitle,
                    'og:description': articleDesc,
                    'og:image': articleImg
    
                    }
                })
            },
            function (response) {
                // Action after response
            });
        })
        $('.odx-share-url__anchor').on('click', function (e) {
            e.preventDefault();
            var url = getShortUrl();
            $(this).attr('data-clipboard-text', url);
            var clipboard = new ClipboardJS('.odx-share-url__anchor');
            clipboard.on('success', function (e) {
            $(e.trigger).html('<span class="odx-share-url__anchor"><span class="odx-share-this__social--icon--copy"><span class="share__icon--url-copy-text">URL Copied!</span></span></span>');
            setTimeout(function(){
                $(e.trigger).html('<a href="" style="text-decoration: none;"><span class="odx-share-this__social--icon"><span class="sr-only">Copy to URL</span><i class="fa fa-link" aria-hidden="true"></i></span><span class="odx-share-this__social--link hidden-xs hidden-md hidden-lg">Copy to URL</span></a>')
            },3000);
            });
            clipboard.on('error', function (e) {
            });
        })
    });
})(jQuery);
