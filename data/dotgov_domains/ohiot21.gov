<script type="text/javascript">
(function($) {
  $(document).ready(function() {
    var $mainContainer = $('#js-odx-content__body');
    var $headings = $mainContainer.find('h2');
    
    for (var i = 0, length = $headings.length; i < length; i++) {
        var $currentHeading = $($headings[i]);
        var wrapperDiv = $currentHeading.parent('div');
        if (wrapperDiv.length && $mainContainer != wrapperDiv) $currentHeading.unwrap();
    }

    function selectDisplayFormat(unprocessedAuthorSelectedDisplayFormat){
      var authorSelectedDisplayFormat = "asTyped";
      switch (unprocessedAuthorSelectedDisplayFormat) {
        case '':
          authorSelectedDisplayFormat = "asTyped";
          break;
        case 'As Typed':
          authorSelectedDisplayFormat = "asTyped";
          break;
        case 'Table of Contents':
          authorSelectedDisplayFormat = "toc";
          break;
        case 'Collapsible Sections':
          authorSelectedDisplayFormat = "twisties";
          break;
        case 'Horizontal Tabs':
          authorSelectedDisplayFormat = "horizontalTabs";
          break;
        case 'Vertical Tabs':
          authorSelectedDisplayFormat = "verticalTabs";
          break;
        case 'Horizontal Tabs with Collapsible Sections':
          authorSelectedDisplayFormat = "horizontalTabsWithCollapsibleSections";
          break;
      }
      return authorSelectedDisplayFormat;
    }
    var authorSelectedDisplayFormat = selectDisplayFormat('Collapsible Sections'),
      innerCollapsibleSections    = selectDisplayFormat('Collapsible Sections');
    var story_transformContent_options = {
      type            : authorSelectedDisplayFormat,
      title           : "Contents",
      initialState    : "closed"
    };
    if (innerCollapsibleSections !== "asTyped") {
      story_transformContent_options["sectionClass"] = 'firstLevelSection';
