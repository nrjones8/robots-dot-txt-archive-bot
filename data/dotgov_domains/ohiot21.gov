        <script>
            (function($) {
                $(document).on('ready', function () {
                    
                    $('#aw-header-top').hide();
                    $('.header').hide();
                    $('.lang-modal__app-wrapper').hide();
                    // 1) Get all variables to support multilingual content
                    var currentLanguage = OHIO.ODX.actions.getUserLanguageByCookie();
    
                    // Get the library according to the user's language
                    var wcmLibrary = (typeof window.WCMLibraries !== "undefined") ? window.WCMLibraries[currentLanguage] : window.WCMLibraries['en'];
                    var virtualPortal = (window.portalContext !== 'none') ? window.portalContext + '/' : 'gov';
    
                    // 2) Configure parameters for AJAX call
                    var params = {
                        source: 'library',
                        srv: 'cmpnt',
                        cmpntid: 'a576c69c-3d9c-4520-a5ef-e5c0f097646b'
                    };
                    var timestamp = new Date();
                    // var serviceURL = '/wps/wcm/connect/gov/odx%20common%20design/utility/context?' + $.param(params) + '&' +  timestamp.getTime();
                    var serviceURL = '/wps/wcm/connect/gov/odx%20common%20design/utility/context?' + $.param(params);
    
    
    var outageNotificationIsCurrent = window.location.pathname.split("/gov/")[1];
    var indexDash = outageNotificationIsCurrent.indexOf("/");
    var cleanURL = outageNotificationIsCurrent.substring(indexDash+1, outageNotificationIsCurrent.length);
    
          if(cleanURL.split("/")[0] != "outage-notification"){
                    // 3) Run the AJAX call and do something with the response
                    OHIO.ODX.actions.getAjaxDataFromURL(serviceURL).then(function (response) {
                        var alertData = JSON.parse(response);
                        var $alert = $('.odx-site-alert');
                        if (!$.isEmptyObject(alertData)) {
    
                            var $icon = $alert.find('.odx-site-alert__icon');
                            var $label = $alert.find('.odx-site-alert__label');
                            var $linkElement = $alert.find('.odx-site-alert__view-more');
                            var alertTypeArray = alertData.alertType.split(',');
                            var indexOfActive = alertTypeArray.indexOf('active-alert');
                                
                            alertTypeArray.splice(indexOfActive, 1);
                            color = alertTypeArray[0];
                            if (color == 'emergency') {
                                $icon.addClass('fa-exclamation-circle');
                                $linkElement.text('Read Full Alert');
                            }
                            if (color == 'news') {
                                $icon.addClass('fa-exclamation-triangle');
                                $linkElement.text('Read Full Notice');
                            }
                            $label.html(alertData.title);
                            $linkElement.attr('href', alertData.url);
                            $alert.addClass('odx-site-alert' + '__alert--' + color);
                            $alert.find('.sr-only').removeClass('sr-only');
                            $alert.find('.no-remove-sr-only').addClass('sr-only');
                            $('.header').show();
                            if(window.siteId == "site"){
                                $('.header').hide();
                            }
                            $('.navigation').addClass('alert-is-visible');
                            $('.lang-modal__app-wrapper').addClass('alert-is-visible');
                            $('#aw-header-top-right').addClass('alert-is-visible');
                            $('.content-body').addClass('alert-is-visible');
                            $('#aw-header-top').show();
                            $('.lang-modal__app-wrapper').show();
                        } else {
                            $alert.remove();
                            
                            $('#aw-header-top').show();
                            $('.header').show();
                            if(window.siteId == "site"){
                                $('.header').hide();
                            }
                            $('.lang-modal__app-wrapper').show();
                        }
    
                    },
                        function (error) {
                            console.error(error);
                        }
                    );
       }
                });
    
            })(jQuery);
    </script>
    
